

@model WebEvento.ViewModel.InformationViewModel
@{
    ViewData["Title"] = "تعداد صاحبان رویداد";
    Layout = "~/Views/Shared/_Layout.cshtml";
}



@if (@ViewBag.Count == null)
{
    <h3>  تعدادکاربران اپ  :  0   </h3>
}
else
{
    <h3>@ViewBag.Count; : تعداد کاربران اپ </h3>

}









<table class="table table-striped table-bordered table-sm" id="dtBasicExample" cellspacing="0" width="100%">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Users.KeyUserApp)
            </th>

         









        </tr>
    </thead>
    <tbody>
        @if (ViewBag.Error == true)
        {

            @foreach (var item in Model.UsersList)
            {
                <tr>
                    <td>
                        @Html.DisplayFor(modelItem => item.KeyUserApp)
                    </td>
              





                </tr>
            }}
        @if (ViewBag.Error == false)

        {
            <tr>
                <td>
                    رویدادی ثبت نشده است
                </td>
            </tr>
        }

    </tbody>
</table>

@section Script{


    <script type="text/javascript">
        $(document).ready(function () {
            $('#dtBasicExample').DataTable();
            $('.dataTables_length').addClass('bs-select');
        });
    </script>



}
