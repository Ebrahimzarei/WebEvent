

@model WebEvento.ViewModel.InformationViewModel
@{
    ViewData["Title"] = "رویداد های منتشر نشده";
    Layout = "~/Views/Shared/_Layout.cshtml";
}



@if (@ViewBag.Count == null)
{
    <h3>  رویداد های منشر نشده  :  0   </h3>
}
else
{
    <h3>@ViewBag.Count; : رویداد های منتشر نشده </h3>

}









<table class="table table-striped table-bordered table-sm" id="dtBasicExample" cellspacing="0" width="100%">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.NotPublished.NameEvent)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.NotPublished.DeleteDate)
            </th>

            <th>
                وضعیت ثبت
            </th>
        </tr>
    </thead>
    <tbody>
        @if (ViewBag.Error == true)
        {

            @foreach (var item in Model.NotPublishedList)
            {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.NameEvent)
            </td>

            <td>
                @Html.DisplayFor(modelItem => item.DeleteDate)
            </td>
            <td class="form-control btn-danger">
                @*@Html.DisplayFor(modelItem => item.Statusevent.GetDisplayName())*@
                رویداد های منتشر نشده
            </td>

        </tr>
            }}
        @if (ViewBag.Error == false)

        {
            <tr>
                <td>
                    رویدادی ثبت نشده است
                </td>
            </tr>
        }

    </tbody>
</table>

@section Script{


    <script type="text/javascript">
        $(document).ready(function () {
            $('#dtBasicExample').DataTable();
            $('.dataTables_length').addClass('bs-select');
        });
    </script>



}
